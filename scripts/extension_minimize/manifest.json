{
  "manifest_version": 3,
  "name": "Minimize Notifier",
  "version": "1.0",
  "permissions": ["windows", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "http://127.0.0.1:8000/*"
  ],
  "action": {
    "default_title": "Minimize Monitor"
  }
}
